<ul class="card-text font-weight-light list-group list-group-flush">
  {% for content in data[1] %}
    <li class="list-group-item">
      <div class="row">
        <div class="col-xs-12 cl-sm-12 col-md-12 mt-2 mt-md-0" style="padding-left: 0;">
          <h6 class="title font-weight-bold ml-2">
            {{ content.title }}
          </h6>
          <h6 class="ml-2">{{ content.awarder }} Â· {{ content.date }}</h6>
          <h6 class="ml-2" style="font-size: 0.95rem;">{{ content.summary }}</h6>
          <h6 class="ml-2" style="font-size: 0.95rem; font-style: italic"><a href="{{ content.url }}">{{ content.url }}</a></h6>
        </div>
      </div>
    </li>
  {% endfor %}
</ul>
